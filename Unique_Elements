#include <stdio.h>
int main() {
    int a[10]={2,1,3,2,4,1,5,5};
    int n=8;
    for(int i=0;i<n;i++){
        int c=0;
        for(int j=0;j<n;j++){
            if(a[i]==a[j]){
                c++;
            }
        }
        if(c==1){
            printf("%d ",a[i]);
        }
    }
}
----------------------------------------------------------------------------------------------------------------------------------------------------
Output:
Final Array is= 3  4 
