#include <stdio.h>
int main() {
   int a[5]={1,2,3,4,5},n=5;
   int i=0,j=n-1,temp;
   while(i<j){
       temp=a[i];
       a[i]=a[j];
       a[j]=temp;
       i++;
       j--;
   }
   printf("Reversed Array is=");
   for(int i=0;i<n;i++){
       printf(" %d",a[i]);
   }
}
--------------------------------------------------------------------------------------------------------------------------------------
Output:
  Reversed Array is= 5 4 3 2 1
